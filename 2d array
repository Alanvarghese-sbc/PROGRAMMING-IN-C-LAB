#include <stdio.h>
int main(void)
{
  int a[10][10],m,n,i,j;
  printf("Enter the number of rows: ");
  scanf("%d",&m);
  printf("Enter the number of columns :");
  scanf("%d",&n);
  printf("Enter the %d  %d matrix elemets :\n",m,n);
  for(i=0;i<m;i++)
  {
    for(j=0;j<n;j++)
      {
        scanf("%d",&a[i][j]);
      }
  }
  printf("MATRIX\n");
   for(i=0;i<m;i++)
  {
    for(j=0;j<n;j++)
      {
        printf("%d\t",a[i][j]);
      }
    printf("\n");
  }
}
